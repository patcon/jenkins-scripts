pipeline {
    agent any
    parameters { string(name: 'SCRIPT_NAME', defaultValue: 'noop.py', description: '') }

    stages {
        stage('Build') {
            steps {
                echo 'Building..'
            }
        }
        stage('Test') {
            steps {
                echo 'Testing..'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying....'
            }
        }
    }
}
